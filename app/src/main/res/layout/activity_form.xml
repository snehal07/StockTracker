<?xml version="1.0" encoding="utf-8"?>
    <RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="snehal.com.stocktracker.Form"
    android:background="#ffffff"
    android:weightSum="1"
    android:orientation="vertical"
    android:baselineAligned="false">

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:paddingTop="2dp"
        android:text="Fill the details"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="16dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignTop="@+id/papersize"
        android:elegantTextHeight="false"
        android:paddingTop="10dp"
        android:text="Paper size"
        android:textColor="#050505"
        android:textSize="16dp"
        tools:layout_editor_absoluteX="6dp"
        tools:layout_editor_absoluteY="53dp" />

    <AutoCompleteTextView
        android:id="@+id/papersize"
        android:layout_width="215dp"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignStart="@+id/companyname"
        android:layout_below="@+id/textView"
        tools:layout_editor_absoluteX="121dp"
        tools:layout_editor_absoluteY="37dp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/papersize"
        android:paddingTop="10dp"
        android:text="Company Name"
        android:textColor="#040404"
        android:textSize="16dp"
        tools:layout_editor_absoluteX="3dp"
        tools:layout_editor_absoluteY="101dp" />

    <AutoCompleteTextView
        android:id="@+id/companyname"
        android:layout_width="213dp"
        android:layout_height="41dp"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/textView3"
        android:layout_toEndOf="@+id/textView3"
        android:inputType="textAutoComplete"
        tools:layout_editor_absoluteX="123dp"
        tools:layout_editor_absoluteY="92dp" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/companyname"
        android:paddingTop="10dp"
        android:text="Paper Type"
        android:textColor="#030303"
        android:textSize="16dp"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="157dp" />

    <AutoCompleteTextView
        android:id="@+id/papertype"
        android:layout_width="213dp"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/companyname"
        android:layout_alignStart="@+id/companyname"
        android:layout_alignTop="@+id/textView4"
        tools:layout_editor_absoluteX="123dp"
        tools:layout_editor_absoluteY="143dp" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/papertype"
        android:paddingTop="10dp"
        android:text="Paper GSM"
        android:textColor="#050505"
        android:textSize="16dp"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="205dp" />

    <AutoCompleteTextView
        android:id="@+id/papergsm"
        android:layout_width="224dp"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/papertype"
        android:layout_alignStart="@+id/papertype"
        android:layout_alignTop="@+id/textView5"
        android:inputType="number"
        tools:layout_editor_absoluteX="112dp"
        tools:layout_editor_absoluteY="205dp" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/papergsm"
        android:paddingTop="10dp"
        android:text="Paper Color"
        android:textColor="#040303"
        android:textSize="16dp"
        tools:layout_editor_absoluteX="2dp"
        tools:layout_editor_absoluteY="267dp" />

    <AutoCompleteTextView
        android:id="@+id/papercolor"
        android:layout_width="224dp"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/papergsm"
        android:layout_alignStart="@+id/papergsm"
        android:layout_below="@+id/papergsm"
        tools:layout_editor_absoluteX="112dp"
        tools:layout_editor_absoluteY="267dp" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/papercolor"
        android:paddingTop="10dp"
        android:text="Quantity"
        android:textColor="#040303"
        android:textSize="16dp"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="326dp" />

    <EditText
        android:id="@+id/paperqty"
        android:layout_width="222dp"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/papercolor"
        android:layout_alignStart="@+id/papercolor"
        android:layout_below="@+id/papercolor"
        android:inputType="number"
        tools:layout_editor_absoluteX="114dp"
        tools:layout_editor_absoluteY="326dp" />

    <Button
        android:id="@+id/buttonDone"
        style="@style/Base.TextAppearance.AppCompat.Button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:background="#2877de"
        android:text="Done"
        android:textColor="#ffffff"
        tools:layout_editor_absoluteX="128dp"
        tools:layout_editor_absoluteY="417dp" />

    ;'=

</RelativeLayout>
