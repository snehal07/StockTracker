<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="snehal.com.stocktracker.ViewActivity"
    android:background="#fdfcfc">

    <TextView
        android:id="@+id/textView20"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/textView24"
        android:layout_alignParentTop="true"
        android:layout_marginTop="17dp"
        android:paddingTop="2dp"
        android:text="Fill the details"
        tools:layout_editor_absoluteX="4dp"
        tools:layout_editor_absoluteY="16dp" />

    <TextView
        android:id="@+id/textView21"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/papersize"
        android:layout_alignBottom="@+id/papersize"
        android:layout_alignEnd="@+id/textView20"
        android:elegantTextHeight="false"
        android:paddingTop="10dp"
        android:text="Paper size"
        android:textColor="#050505"
        android:textSize="16dp"
        tools:layout_editor_absoluteX="7dp"
        tools:layout_editor_absoluteY="58dp" />

    <AutoCompleteTextView
        android:id="@+id/papersize"
        android:layout_width="225dp"
        android:layout_height="wrap_content"
        android:layout_above="@+id/companyname"
        android:layout_alignParentEnd="true"
        android:layout_marginBottom="14dp"
        tools:layout_editor_absoluteX="160dp"
        tools:layout_editor_absoluteY="53dp" />

    <TextView
        android:id="@+id/textView22"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/companyname"
        android:layout_alignBottom="@+id/companyname"
        android:layout_alignParentStart="true"
        android:paddingTop="10dp"
        android:text="Company Name"
        android:textColor="#040404"
        android:textSize="16dp"
        tools:layout_editor_absoluteX="5dp"
        tools:layout_editor_absoluteY="122dp" />

    <AutoCompleteTextView
        android:id="@+id/companyname"
        android:layout_width="226dp"
        android:layout_height="42dp"
        android:layout_above="@+id/papertype"
        android:layout_alignStart="@+id/papertype"
        android:layout_marginBottom="13dp"
        tools:layout_editor_absoluteX="158dp"
        tools:layout_editor_absoluteY="115dp" />

    <TextView
        android:id="@+id/textView23"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/papertype"
        android:layout_alignBottom="@+id/papertype"
        android:layout_alignEnd="@+id/textView25"
        android:paddingTop="10dp"
        android:text="Paper Type"
        android:textColor="#030303"
        android:textSize="16dp"
        tools:layout_editor_absoluteX="11dp"
        tools:layout_editor_absoluteY="183dp" />

    <AutoCompleteTextView
        android:id="@+id/papertype"
        android:layout_width="258dp"
        android:layout_height="wrap_content"
        android:layout_above="@+id/papergsm"
        android:layout_alignStart="@+id/papergsm"
        android:layout_marginBottom="14dp"
        tools:layout_editor_absoluteX="158dp"
        tools:layout_editor_absoluteY="170dp" />

    <TextView
        android:id="@+id/textView24"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/papergsm"
        android:layout_alignBottom="@+id/papergsm"
        android:layout_alignStart="@+id/textView25"
        android:paddingTop="10dp"
        android:text="Paper GSM"
        android:textColor="#050505"
        android:textSize="16dp"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="239dp" />

    <AutoCompleteTextView
        android:id="@+id/papergsm"
        android:layout_width="244dp"
        android:layout_height="wrap_content"
        android:layout_above="@+id/papercolor"
        android:layout_alignStart="@+id/papercolor"
        android:layout_marginBottom="14dp"
        tools:layout_editor_absoluteX="156dp"
        tools:layout_editor_absoluteY="232dp" />

    <TextView
        android:id="@+id/textView25"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/papercolor"
        android:layout_alignEnd="@+id/textView22"
        android:layout_marginEnd="14dp"
        android:paddingTop="10dp"
        android:text="Paper Color"
        android:textColor="#040303"
        android:textSize="16dp"
        tools:layout_editor_absoluteX="2dp"
        tools:layout_editor_absoluteY="295dp" />

    <AutoCompleteTextView
        android:id="@+id/papercolor"
        android:layout_width="230dp"
        android:layout_height="39dp"
        android:layout_above="@+id/buttonDone"
        android:layout_alignParentEnd="true"
        android:layout_marginBottom="55dp"
        tools:layout_editor_absoluteX="154dp"
        tools:layout_editor_absoluteY="294dp" />

    <Button
        android:id="@+id/buttonDone"
        style="@style/Widget.AppCompat.Button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignStart="@+id/papercolor"
        android:layout_marginBottom="36dp"
        android:background="#2877de"
        android:text="Done"
        android:textColor="@android:color/background_light"
        tools:layout_editor_absoluteX="119dp"
        tools:layout_editor_absoluteY="389dp" />
</RelativeLayout>
